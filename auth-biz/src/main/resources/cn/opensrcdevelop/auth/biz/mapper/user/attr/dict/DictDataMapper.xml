<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.opensrcdevelop.auth.biz.mapper.user.attr.dict.DictDataMapper">

    <select id="getNextDisplaySeq" resultType="Integer">
        SELECT COALESCE
               ( MAX ( display_seq ) + 1, 1 )
        FROM
            t_dict_data
        WHERE
            dict_id = #{dictId}
            AND deleted = false
    </select>
</mapper>